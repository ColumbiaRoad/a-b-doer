{
  "name": "a-b-doer",
  "version": "5.0.0-beta",
  "description": "Utility library which makes developing of A/B test variants easier (maybe)",
  "main": "./main.js",
  "scripts": {
    "build": "rollup ./bin/abdo.js -c",
    "clean": "rimraf ./dist",
    "try:build": "node ./dist/bin/abdo.js build",
    "try:build-all": "node ./dist/bin/abdo.js build-all",
    "try:watch": "node ./dist/bin/abdo.js watch",
    "try:preview": "node ./dist/bin/abdo.js preview",
    "try:screenshot": "node ./dist/bin/abdo.js screenshot",
    "test": "npm run test:js -- --run; npm run test:styles -- --run; npm run test:config -- --run",
    "test:js": "vitest -c ./test/js/vitest.config.js test/js",
    "test:styles": "vitest -c ./test/styles/vitest.config.js  test/styles",
    "test:config": "vitest -c ./test/config/vitest.config.js test/config",
    "release": "standard-version",
    "prepare": "husky install"
  },
  "author": "Timo Partti <timo.partti@columbiaroad.com>",
  "license": "MIT",
  "keywords": [
    "ab-testing",
    "ab-test",
    "variants",
    "variant-testing"
  ],
  "dependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.4",
    "@babel/plugin-transform-runtime": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-typescript": "^7.21.4",
    "@prefresh/babel-plugin": "^0.4.4",
    "@prefresh/vite": "^2.2.9",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/pluginutils": "^5.0.2",
    "@vitejs/plugin-basic-ssl": "^1.0.1",
    "babel-plugin-transform-async-to-promises": "^0.8.18",
    "babel-plugin-transform-import-meta": "^2.2.0",
    "browserslist": "^4.21.5",
    "chalk": "^5.2.0",
    "chokidar": "^3.5.3",
    "core-js": "^3.30.0",
    "dotenv": "^16.0.3",
    "esbuild": "^0.17.15",
    "glob": "^9.3.4",
    "less": "^4.1.3",
    "lodash.get": "^4.4.2",
    "lodash.ismatch": "^4.4.0",
    "lodash.set": "^4.3.2",
    "minimist": "^1.2.8",
    "mkdirp": "^2.1.6",
    "preact": "^10.13.2",
    "puppeteer-core": "^19.8.3",
    "rimraf": "^4.4.1",
    "rollup-plugin-inline-svg": "^3.0.3",
    "rollup-plugin-node-externals": "^5.1.2",
    "rollup-plugin-svg-hyperscript": "^0.1.2",
    "sass": "^1.60.0",
    "string-hash": "^1.1.3",
    "terser": "^5.16.8",
    "tslib": "^2.5.0",
    "typescript": "^5.0.3",
    "vite": "^4.2.1",
    "vite-plugin-svgr": "^2.4.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^9.2.0",
    "@testing-library/jest-dom": "^5.16.5",
    "babel-jest": "^29.5.0",
    "eslint": "^8.37.0",
    "eslint-config-preact": "^1.3.0",
    "eslint-plugin-react": "^7.32.2",
    "expect-puppeteer": "^8.0.5",
    "glob": "^9.3.4",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-fail-on-console": "^3.1.1",
    "jsdom": "^21.1.1",
    "puppeteer": "^19.8.3",
    "rimraf": "^4.4.1",
    "standard-version": "^9.5.0",
    "vitest": "^0.29.8"
  },
  "bin": {
    "abdo": "dist/bin/abdo.js"
  },
  "files": [
    "dist",
    "lib",
    "src",
    "index.d.ts",
    "hooks.js",
    "hooks.d.ts",
    "bundler.js",
    "bundler.d.ts",
    "main.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ColumbiaRoad/a-b-doer.git"
  },
  "bugs": {
    "url": "https://github.com/ColumbiaRoad/a-b-doer/issues"
  },
  "homepage": "https://github.com/ColumbiaRoad/a-b-doer#readme",
  "type": "module",
  "engines": {
    "node": ">=16.0.0"
  },
  "exports": {
    ".": "./main.js",
    "./hooks": "./hooks.js",
    "./bundler": "./bundler.js"
  }
}
