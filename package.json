{
  "name": "a-b-doer",
  "version": "5.0.0-beta",
  "description": "Utility library which makes developing of A/B test variants easier (maybe)",
  "main": "./main.js",
  "scripts": {
    "build": "rollup ./bin/abdo.js -c",
    "clean": "rimraf ./dist",
    "try:build": "node ./dist/bin/abdo.js build",
    "try:build-all": "node ./dist/bin/abdo.js build-all",
    "try:watch": "node ./dist/bin/abdo.js watch",
    "try:preview": "node ./dist/bin/abdo.js preview",
    "try:screenshot": "node ./dist/bin/abdo.js screenshot",
    "test": "jest --runInBand",
    "release": "standard-version",
    "prepare": "husky install"
  },
  "author": "Timo Partti <timo.partti@columbiaroad.com>",
  "license": "MIT",
  "keywords": [
    "ab-testing",
    "ab-test",
    "variants",
    "variant-testing"
  ],
  "dependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.5",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@rollup/plugin-replace": "^5.0.1",
    "@rollup/pluginutils": "^5.0.2",
    "@vitejs/plugin-basic-ssl": "^0.1.2",
    "babel-plugin-transform-async-to-promises": "^0.8.18",
    "babel-plugin-transform-import-meta": "^2.2.0",
    "browserslist": "^4.21.4",
    "chalk": "^5.1.2",
    "chokidar": "^3.5.3",
    "core-js": "^3.26.1",
    "dotenv": "^16.0.3",
    "esbuild": "^0.16.1",
    "glob": "^8.0.3",
    "less": "^4.1.3",
    "lodash.get": "^4.4.2",
    "lodash.merge": "^4.6.2",
    "lodash.set": "^4.3.2",
    "minimist": "^1.2.7",
    "preact": "^10.11.3",
    "puppeteer-core": "^19.4.0",
    "rimraf": "^3.0.2",
    "rollup-plugin-inline-svg": "^3.0.3",
    "rollup-plugin-node-externals": "^5.0.2",
    "rollup-plugin-svg-hyperscript": "^0.1.2",
    "sass": "^1.56.1",
    "string-hash": "^1.1.3",
    "terser": "^5.16.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4",
    "vite": "^3.2.5",
    "vite-plugin-svgr": "^2.2.2"
  },
  "devDependencies": {
    "babel-jest": "^29.3.1",
    "expect-puppeteer": "^6.1.1",
    "glob": "^8.0.3",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "jest-fail-on-console": "^3.0.2",
    "puppeteer": "^19.4.0",
    "rimraf": "^3.0.2",
    "standard-version": "^9.5.0"
  },
  "bin": {
    "abdo": "dist/bin/abdo.js"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./test/setup.js"
    ],
    "testEnvironment": "./test/puppeteer-environment.js",
    "globalSetup": "./test/global-setup.js",
    "globalTeardown": "./test/global-teardown.js",
    "modulePathIgnorePatterns": [
      ".build"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!(chalk|ansi-styles)/)"
    ],
    "moduleNameMapper": {
      "#(.*)": "<rootDir>/node_modules/$1"
    },
    "transform": {
      "\\.jsx?$": [
        "babel-jest",
        {
          "presets": [
            "@babel/preset-env"
          ],
          "plugins": [
            "@babel/transform-runtime",
            "babel-plugin-transform-import-meta"
          ]
        }
      ]
    }
  },
  "files": [
    "dist",
    "lib",
    "src",
    "index.d.ts",
    "hooks.js",
    "hooks.d.ts",
    "main.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ColumbiaRoad/a-b-doer.git"
  },
  "bugs": {
    "url": "https://github.com/ColumbiaRoad/a-b-doer/issues"
  },
  "homepage": "https://github.com/ColumbiaRoad/a-b-doer#readme",
  "type": "module",
  "engines": {
    "node": ">=14.18.0"
  }
}
