{
  "name": "a-b-doer",
  "version": "5.0.0-beta",
  "description": "Utility library which makes developing of A/B test variants easier (maybe)",
  "main": "./main.js",
  "scripts": {
    "build": "rollup ./bin/abdo.js -c",
    "clean": "rimraf ./dist",
    "try:build": "node ./dist/bin/abdo.js build",
    "try:build-all": "node ./dist/bin/abdo.js build-all",
    "try:watch": "node ./dist/bin/abdo.js watch",
    "try:preview": "node ./dist/bin/abdo.js preview",
    "try:screenshot": "node ./dist/bin/abdo.js screenshot",
    "test": "vitest",
    "test:js": "vitest -c ./test/js/vitest.config.js test/js",
    "test:styles": "vitest test/styles",
    "release": "standard-version",
    "prepare": "husky install"
  },
  "author": "Timo Partti <timo.partti@columbiaroad.com>",
  "license": "MIT",
  "keywords": [
    "ab-testing",
    "ab-test",
    "variants",
    "variant-testing"
  ],
  "dependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@prefresh/babel-plugin": "^0.4.4",
    "@prefresh/vite": "^2.2.9",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/pluginutils": "^5.0.2",
    "@vitejs/plugin-basic-ssl": "^1.0.1",
    "babel-plugin-transform-async-to-promises": "^0.8.18",
    "babel-plugin-transform-import-meta": "^2.2.0",
    "browserslist": "^4.21.4",
    "chalk": "^5.2.0",
    "chokidar": "^3.5.3",
    "core-js": "^3.27.1",
    "dotenv": "^16.0.3",
    "esbuild": "^0.17.2",
    "glob": "^8.1.0",
    "less": "^4.1.3",
    "lodash.get": "^4.4.2",
    "lodash.set": "^4.3.2",
    "minimist": "^1.2.7",
    "mkdirp": "^2.1.3",
    "preact": "^10.11.3",
    "puppeteer-core": "^19.5.2",
    "rimraf": "^4.1.1",
    "rollup-plugin-inline-svg": "^3.0.3",
    "rollup-plugin-node-externals": "^5.1.0",
    "rollup-plugin-svg-hyperscript": "^0.1.2",
    "sass": "^1.57.1",
    "string-hash": "^1.1.3",
    "terser": "^5.16.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vite-plugin-svgr": "^2.4.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^8.20.0",
    "@testing-library/jest-dom": "^5.16.5",
    "babel-jest": "^29.3.1",
    "eslint": "^8.32.0",
    "eslint-config-preact": "^1.3.0",
    "eslint-plugin-react": "^7.32.1",
    "expect-puppeteer": "^6.1.1",
    "glob": "^8.1.0",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "jest-fail-on-console": "^3.0.2",
    "jsdom": "^21.0.0",
    "puppeteer": "^19.5.2",
    "rimraf": "^4.1.1",
    "standard-version": "^9.5.0",
    "vitest": "^0.27.2"
  },
  "bin": {
    "abdo": "dist/bin/abdo.js"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./test/setup.js"
    ],
    "testEnvironment": "./test/puppeteer-environment.js",
    "globalSetup": "./test/global-setup.js",
    "globalTeardown": "./test/global-teardown.js",
    "modulePathIgnorePatterns": [
      ".build"
    ],
    "moduleNameMapper": {
      "#(.*)": "<rootDir>/node_modules/$1"
    },
    "transform": {}
  },
  "files": [
    "dist",
    "lib",
    "src",
    "index.d.ts",
    "hooks.js",
    "hooks.d.ts",
    "bundler.js",
    "bundler.d.ts",
    "main.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ColumbiaRoad/a-b-doer.git"
  },
  "bugs": {
    "url": "https://github.com/ColumbiaRoad/a-b-doer/issues"
  },
  "homepage": "https://github.com/ColumbiaRoad/a-b-doer#readme",
  "type": "module",
  "engines": {
    "node": ">=16.0.0"
  }
}
