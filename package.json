{
  "name": "a-b-doer",
  "version": "1.0.0",
  "description": "Utility library which makes developing of A/B test variants easier (maybe)",
  "main": "index.js",
  "scripts": {
    "build": "node ./scripts/abdo.js build",
    "build-all": "node ./scripts/abdo.js build-all",
    "watch": "node ./scripts/abdo.js watch",
    "preview": "node ./scripts/abdo.js preview",
    "test": "jest --runInBand"
  },
  "author": "Timo Partti <timo.partti@columbiaroad.com>",
  "license": "MIT",
  "keywords": [
    "ab-testing",
    "ab-test",
    "variants",
    "variant-testing"
  ],
  "dependencies": {
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.12.8",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.7",
    "@babel/plugin-transform-react-jsx": "^7.12.7",
    "@babel/preset-env": "^7.12.7",
    "@emotion/babel-plugin-jsx-pragmatic": "^0.1.5",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-image": "^2.0.5",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-replace": "^2.3.4",
    "autoprefixer": "^10.0.2",
    "babel-plugin-transform-async-to-promises": "^0.8.15",
    "chalk": "^4.1.0",
    "clsx": "^1.1.1",
    "core-js": "^3.7.0",
    "less": "^4.0.0",
    "node-sass": "^5.0.0",
    "postcss": "^8.1.10",
    "postcss-modules": "^3.2.2",
    "preact": "^10.5.7",
    "puppeteer-core": "^5.5.0",
    "rollup": "^2.33.3",
    "rollup-plugin-ejs": "^2.2.0",
    "rollup-plugin-inline-svg": "^2.0.0",
    "rollup-plugin-styles": "^3.11.0",
    "rollup-plugin-svg-hyperscript": "^0.1.2",
    "rollup-plugin-terser": "^7.0.2",
    "string-hash": "^1.1.3"
  },
  "devDependencies": {
    "glob": "^7.1.6",
    "jest": "^26.6.3",
    "puppeteer": "^5.5.0",
    "rimraf": "^3.0.2"
  },
  "bin": {
    "abdo": "scripts/abdo.js"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./jest/setup.js"
    ],
    "testEnvironment": "./jest/puppeteer-environment.js",
    "globalSetup": "./jest/global-setup.js",
    "globalTeardown": "./jest/global-teardown.js"
  },
  "directories": {
    "lib": "lib",
    "scripts": "scripts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ColumbiaRoad/a-b-doer.git"
  },
  "bugs": {
    "url": "https://github.com/ColumbiaRoad/a-b-doer/issues"
  },
  "homepage": "https://github.com/ColumbiaRoad/a-b-doer#readme"
}
