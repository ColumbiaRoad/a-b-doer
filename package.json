{
  "name": "a-b-doer",
  "version": "3.1.0",
  "description": "Utility library which makes developing of A/B test variants easier (maybe)",
  "main": "./main.js",
  "scripts": {
    "build": "rollup ./bin/abdo.js -c",
    "clean": "rimraf ./dist",
    "try:build": "node ./dist/bin/abdo.js build",
    "try:build-all": "node ./dist/bin/abdo.js build-all",
    "try:watch": "node ./dist/bin/abdo.js watch",
    "try:preview": "node ./dist/bin/abdo.js preview",
    "try:screenshot": "node ./dist/bin/abdo.js screenshot",
    "test": "jest --runInBand",
    "release": "standard-version",
    "prepare": "husky install"
  },
  "author": "Timo Partti <timo.partti@columbiaroad.com>",
  "license": "MIT",
  "keywords": [
    "ab-testing",
    "ab-test",
    "variants",
    "variant-testing"
  ],
  "dependencies": {
    "@babel/cli": "^7.16.8",
    "@babel/core": "^7.16.12",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.16.7",
    "@babel/plugin-proposal-optional-chaining": "^7.16.7",
    "@babel/plugin-transform-react-jsx": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.16.10",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@emotion/babel-plugin-jsx-pragmatic": "^0.1.5",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-image": "^2.1.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-replace": "^3.0.1",
    "autoprefixer": "^10.4.2",
    "babel-plugin-transform-async-to-promises": "^0.8.18",
    "babel-plugin-transform-import-meta": "^2.1.0",
    "chalk": "^5.0.0",
    "chokidar": "^3.5.3",
    "core-js": "^3.20.3",
    "dotenv": "^14.3.2",
    "less": "^4.1.2",
    "lodash.merge": "^4.6.2",
    "minimist": "^1.2.5",
    "node-sass": "^7.0.1",
    "postcss": "^8.4.5",
    "postcss-modules": "^4.3.0",
    "preact": "^10.6.5",
    "puppeteer-core": "^13.1.2",
    "rollup": "^2.66.1",
    "rollup-plugin-iife": "^0.5.0",
    "rollup-plugin-inline-svg": "^2.0.0",
    "rollup-plugin-node-externals": "^3.1.2",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-styles": "^4.0.0",
    "rollup-plugin-svg-hyperscript": "^0.1.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.1",
    "string-hash": "^1.1.3",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
  },
  "devDependencies": {
    "babel-jest": "^27.4.6",
    "expect-puppeteer": "^6.0.2",
    "glob": "^7.2.0",
    "husky": "^7.0.4",
    "jest": "^27.4.7",
    "jest-fail-on-console": "^2.1.1",
    "puppeteer": "^13.1.2",
    "rimraf": "^3.0.2",
    "standard-version": "^9.3.2"
  },
  "bin": {
    "abdo": "dist/bin/abdo.js"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./test/setup.js"
    ],
    "testEnvironment": "./test/puppeteer-environment.js",
    "globalSetup": "./test/global-setup.js",
    "globalTeardown": "./test/global-teardown.js",
    "modulePathIgnorePatterns": [
      ".build"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!(chalk|ansi-styles)/)"
    ],
    "moduleNameMapper": {
      "#(.*)": "<rootDir>/node_modules/$1"
    },
    "transform": {
      "\\.jsx?$": [
        "babel-jest",
        {
          "presets": [
            "@babel/preset-env"
          ],
          "plugins": [
            "@babel/transform-runtime",
            "babel-plugin-transform-import-meta"
          ]
        }
      ]
    }
  },
  "files": [
    "dist",
    "lib",
    "src",
    "index.d.ts",
    "hooks.js",
    "hooks.d.ts",
    "main.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ColumbiaRoad/a-b-doer.git"
  },
  "bugs": {
    "url": "https://github.com/ColumbiaRoad/a-b-doer/issues"
  },
  "homepage": "https://github.com/ColumbiaRoad/a-b-doer#readme",
  "type": "module"
}
